{
  "name": "@joule/models",
  "version": "0.1.0",
  "description": "Model provider adapters for Joule - Ollama, Anthropic, OpenAI, Google",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joule-ai/joule",
    "directory": "packages/models"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "joule",
    "ai-agent",
    "ollama",
    "anthropic",
    "openai",
    "google",
    "llm-provider"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.1",
    "@joule/shared": "workspace:*",
    "openai": "^4.80.0"
  }
}
